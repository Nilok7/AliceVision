{
  "name": "alicevision",
  "version-string": "2.4.0",
  "port-version": 1,
  "description": "photogrammetric-computer-vision-framework",
  "dependencies": [
    {
      "name": "atlmfc",
      "platform": "windows"
    },
    "boost-algorithm",
    "boost-accumulators",
    "boost-atomic",
    "boost-container",
    "boost-date-time",
    "boost-exception",
    "boost-filesystem",
    "boost-graph",
    "boost-log",
    "boost-program-options",
    "boost-property-tree",
    "boost-ptr-container",
    "boost-regex",
    "boost-serialization",
    "boost-system",
    "boost-test",
    "boost-thread",
    "boost-timer",
    "lz4",
    "openexr",
    "alembic",
    "geogram",
    "eigen3",
    {
      "name": "opencv",
      "features": [ "eigen", "ffmpeg", "webp", "contrib", "nonfree", "cuda" ],
      "platform": "windows"
    },
    {
      "name": "openimageio",
      "features": [ "libraw", "ffmpeg", "freetype", "opencv", "gif", "openjpeg", "webp", "tools" ],
      "platform": "windows"
    },
    {
      "name": "ceres",
      "features": [ "suitesparse", "cxsparse" ],
      "platform": "windows"
    },
    "cuda",
    "tbb"
  ]
} 
